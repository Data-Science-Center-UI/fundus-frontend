<%= turbo_frame_tag 'users' do %>
    <div class="table__container">
        <table class="table" data-controller="partials--result-table">
            <thead class="table__header">
                <tr>
                    <th>Fullname</th>
                    <th>Username</th>
                    <th>Email</th>
                    <th>Role</th>
                    <th>Status</th>
                </tr>
            </thead>
            <tbody class="table__body zebra-row" id="user-data" data-controller="components--table">
                <%#= render user_list_data_template(users, collection: true) %>
            </tbody>
        </table>
    </div>
<% end %>
<%= render load_more_template dashboard_users_lists_path(page: 1, format: :turbo_stream) %>
